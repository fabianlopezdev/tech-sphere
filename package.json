{
  "dependencies": {
    "astro": "^5.7.10",
    "three": "^0.176.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.1",
    "@testing-library/dom": "^10.4.0",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsonc": "^2.20.1",
    "happy-dom": "^17.4.7",
    "husky": "^9.1.7",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "start-server-and-test": "^2.0.12",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,jsonc,json5}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{md}": [
      "prettier --write"
    ]
  },
  "name": "tech-sphere",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,astro,json,css,md}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "a11y": "axe http://localhost:4321 --exit",
    "a11y:ci": "start-server-and-test dev http://localhost:4321 a11y"
  },
  "type": "module",
  "version": "0.0.1"
}
